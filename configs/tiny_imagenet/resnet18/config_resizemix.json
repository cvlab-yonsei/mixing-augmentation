{
    "name": "",
    
    "n_gpu": 1,
    "multiprocessing_distributed": false,
    "rank": 0,
    "world_size": 1,
    "dist_backend": "nccl",
    "dist_url": "tcp://127.0.0.1:12345",
    "seed": 1,
    "use_amp": false,
    "data_loader": {
        "type": "ClassificationDataLoader",
        "args": {
            "dataset": "tiny_imagenet",
            "batch_size": 100,
            "num_workers": 4,
            "pin_memory": true
        }
    },
    "arch": {
        "type": "Mixup",
        "args": {
            "backbone": {
                "type": "ResNet_CIFAR",
                "args": {
                    "depth": 18,
                    "zero_init_residual": true
                }
            },
            "mixing_augmentation": {
                "type": "ResizeMix_m",
                "args":{
                    "distribution": "uniform",
                    "alpha1": 0.1,
                    "alpha2": 0.8,
                    "mix_prob": 0.5
                }
            }
        }
    },
    "optimizer": {
        "type": "SGD",
        "args": {
            "lr": 0.2,
            "weight_decay": 1e-4,
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "lr_scheduler": {
        "type": "CosineAnnealingLR",
        "args": {
            "eta_min": 0.0
        }
    },
    "evaluator": {
        "type": "Evaluator",
        "args": {}
    },
    "metrics": [
        "Average_Accuracy"
    ],
    "trainer": {
        "epochs": 400,
        "save_dir": "saved/",
        "save_period": -1,
        "validation_period": 1,
        "verbosity": 2,
        "monitor": "max val_Average_Accuracy_top1",
        "early_stop": -1,
        "reset_best_mnt": false,
        "tensorboard": false
    }
}